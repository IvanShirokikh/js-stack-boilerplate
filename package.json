{
  "name": "js-stack-from-scratch",
  "version": "2.4.5",
  "repository": "verekia/js-stack-boilerplate",
  "author": "Jonathan Verrecchia - @verekia",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": "7.x",
    "yarn": "0.24.5"
  },
  "scripts": {
    "start": "npm-run-all -p dev:start dev:wds",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "heroku-postbuild": "yarn prod:build",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "4.0.0-alpha.6",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.0",
    "immutable": "^4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "mongoose": "^4.10.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-jss": "^6.1.1",
    "react-redux": "^5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.1",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-flow-react-proptypes": "^2.2.1",
    "babel-preset-env": "^1.5.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.46.0",
    "husky": "^0.13.3",
    "jest": "^20.0.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
